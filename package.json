{
  "name": "preact-parcel-ssr",
  "alias": {
    "react": "preact-compat/react",
    "react-dom": "preact-compat/react-dom"
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/client/index.html --out-dir dist/public",
    "client": "BABEL_ENV=client parcel build src/client/index.html --out-dir dist/public",
    "server":
      "BABEL_ENV=server parcel build src/server/index.js --out-dir dist/server --target=node",
    "build": "npm run client & npm run server",
    "start": "node ./dist/server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "preact": "^8.2.7",
    "preact-render-to-string": "^3.7.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-async-import": "^1.0.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-dynamic-import-node-sync": "^2.0.1",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-preact": "^1.1.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "parcel-bundler": "^1.6.2",
    "preact-compat": "^3.18.0",
    "react-router-dom": "^4.2.2"
  }
}
